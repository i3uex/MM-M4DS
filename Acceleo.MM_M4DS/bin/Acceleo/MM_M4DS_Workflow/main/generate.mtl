			[let d_or : DataProcessing = ps.oclAsType(DataProcessing)]								[comment OPEN assigns the variable to be a DataProcessing/]
				[for (c : Contract | dw.contract)]													[comment OPEN traverse all the contracts in the DataProcessing/]
